page{
  background-color: rgb(250, 250, 250)
}
.viewclass{
    height: 100px;
    width: 98%;
    border: solid 2px rgb(231, 153, 52);
    display: flex;
    flex-direction: row;
    justify-content: center;
    background-color: aqua;
  }

  .start-btn{
    width:100px;
    height:30px;
    background:rgb(66, 223, 87);
    font-size:14px;
    color:rgb(255, 255, 255);
    text-align: center;
  }
  .labelcs{
    padding-top: 5rpx;
    background:rgb(255, 255, 255);
  }
  .table {
    border: 2px solid rgb(97, 96, 96);
    width: 98%;
    height: 90%;
    
    margin: auto;
    margin-top: 20%;
    background-color: rgb(255, 255, 255)
  }
  .tr {
    display: flex;
    width: 100%;
    justify-content: center;
    height: 3rem;
    align-items: center;
  }
  .th {
    width: 100%;
    justify-content: center;
    background: rgb(255, 255, 255);
    color: rgb(248, 147, 147);
    display: flex;
    height: 3rem;
    align-items: center;
  }